{
  "name": "lxd-web-panel-client",
  "version": "1.0.0",
  "private": true,
  "description": "A web panel for LXD",
  "main": "index.js",
  "config": {
    "buildPath": "./dist",
    "defaultAPIHost": "192.168.50.4",
    "defaultAPIPort": 9000,
    "devHost": "localhost",
    "devPort": 3000,
    "srcPath": "./src"
  },
  "scripts": {
    "dev": "npm run dev-server",
    "dev-server": "webpack-dev-server --devtool eval --progress --inline --colors --hot",
    "lint": "eslint --ext .js --ext .jsx ./src && echo No linting errors.",
    "predev": "cd ../ && npm run generate-model-schema && cd client/",
    "prestart": "npm install",
    "start": "NODE_ENV=development API_HOST=$npm_package_config_defaultAPIHost API_PORT=$npm_package_config_defaultAPIPort npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Richard Willis",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "bootstrap-sass": "^3.3.6",
    "classnames": "^2.2.3",
    "flux": "^2.1.1",
    "history": "^1.17",
    "lodash": "^3.10.1",
    "promise.prototype.finally": "^1.0.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^1.0.3",
    "whatwg-fetch": "^0.10.1"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.0.2",
    "eslint-plugin-react": "^3.14.0",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "opn-cli": "^1.0.0",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  }
}
